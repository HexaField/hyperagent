{
  "name": "hyperagent",
  "version": "0.1.0",
  "description": "A self recursive AI agent framework and collaboration workspace",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write .",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts --watch src",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "ui:server": "ts-node-dev --respawn --transpile-only ui/server/index.ts",
    "ui:web": "vite --config ui/web/vite.config.ts",
    "ui:dev": "concurrently -k -n server,web -c green,cyan \"npm:ui:server\" \"npm:ui:web\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^3.1.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "multer": "^2.0.2",
    "ollama": "^0.6.0",
    "pacote": "^21.0.4",
    "solid-js": "^1.9.3",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/pacote": "^11.1.8",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-fetch": "^3.1.8",
    "eslint": "^8.47.0",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tailwindcss": "^3.4.15",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2",
    "vite": "^5.4.10",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18"
  }
}
